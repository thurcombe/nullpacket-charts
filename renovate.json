{
  "extends": ["config:recommended"],
  "regexManagers": [
    {
      "fileMatch": ["^Chart\\.yaml$"],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>[^\\s]+)\\s+depName=(?<depName>[^\\s]+)(?:\\s+versioning=(?<versioning>[^\\s]+))?\\s*\\nappVersion:\\s+\"?(?<currentValue>[^\"\\s]+)\"?"
      ]
    }
  ]
}
