{
  "extends": ["config:recommended"],
  "onboarding": false,
  "regexManagers": [
    {
      "fileMatch": ["^Chart\\.yaml$"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)( versioning=(?<versioning>\\S+))?\\nappVersion: \"(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "{{ datasource }}",
      "depNameTemplate": "{{ depName }}",
      "versioningTemplate": "{{ versioning }}"
    }
  ]
}
  